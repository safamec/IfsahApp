{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build:dev",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/IfsahApp/IfsahApp.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary;ForceNoAlign"
      ],
      "options": {
        "cwd": "${workspaceFolder}/IfsahApp",
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        }
      },
      "problemMatcher": "$msCompile"
    },
