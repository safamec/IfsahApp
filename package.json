{
  "name": "ifsahapp",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "restore": "dotnet restore IfsahApp\\IfsahApp.csproj && powershell -Command \"pushd IfsahApp\\Web; libman restore; popd\"",
    "build": "dotnet build IfsahApp\\IfsahApp.csproj",
    "start": "dotnet run --project IfsahApp\\IfsahApp.csproj",
    "watch": "dotnet watch run --project IfsahApp\\IfsahApp.csproj",
    "lib": "node libman.js",
    "start:dev": "dotnet run --project IfsahApp\\IfsahApp.csproj --launch-profile \"dev\"",
    "start:stag": "dotnet run --project IfsahApp\\IfsahApp.csproj --launch-profile \"stag\"",
    "start:prod": "dotnet run --project IfsahApp\\IfsahApp.csproj --launch-profile \"prod\"",
    "watch:dev": "dotnet watch run --project IfsahApp\\IfsahApp.csproj --launch-profile \"dev\"",
    "watch:stag": "dotnet watch run --project IfsahApp\\IfsahApp.csproj --launch-profile \"stag\"",
    "watch:prod": "dotnet watch run --project IfsahApp\\IfsahApp.csproj --launch-profile \"prod\""
  },
