{
  "Authentication": {
    "UseFakeUser": false
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=app.db"
  },
  "FileUploadSettings": {
    "AllowedExtensions": [
      ".jpg", ".jpeg", ".png", ".gif", ".bmp",
      ".mp4", ".mov", ".avi", ".wmv", ".mkv",
      ".pdf", ".doc", ".docx", ".xls", ".xlsx",
      ".ppt", ".pptx"
    ],
    "MaxFileSize": 10485760
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Error",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
